{
  "AppSettings": {
    "MapboxToken": "YOUR_MAPBOX_TOKEN",
    "GoogleApiToken": "YOUR_GOOGLE_PLACE_TOKEN",

    "GooglePlaceApi": {
      "Nearbysearch": "https://maps.googleapis.com/maps/api/place/nearbysearch/json?key={0}&location={1},{2}&radius={3}&name={4}&type={5}&language={6}",
      "FindFromText": "https://maps.googleapis.com/maps/api/place/findplacefromtext/json?input={1}&inputtype=textquery&fields=photos,formatted_address,name,rating,opening_hours,geometry&key={0}",
      "Detail": "https://maps.googleapis.com/maps/api/place/details/json?key={0}&placeid={1}&language={2}",
      "Photo": "https://maps.googleapis.com/maps/api/place/photo?key={0}&photoreference={1}&maxwidth={2}"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
